<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="SmokeSuite" parallel="classes" thread-count="5">
	<listeners>
		<listener class-name="com.Bstackdemo.listerners.TestListener" />
	</listeners>
	<test name="SmokeTests">
		<groups>
			<run>
				<include name="smoke" />
			</run>
		</groups>
		<packages>
			<package name="com.Bstackdemo.tests" />
		</packages>
	</test>
</suite>
